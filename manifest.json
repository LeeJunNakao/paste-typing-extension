{
    "manifest_version": 3,
    "name": "Paste Typing",
    "version": "1.0",
    "author": "leejun.nakao@gmail.com",
    "permissions": [
        "clipboardWrite",
        "clipboardRead",
        "activeTab",
        "contextMenus"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "icons": {
        "16": "icon.png",
        "32": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    }
}